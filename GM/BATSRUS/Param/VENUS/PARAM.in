!- PARAM.in:  Example input file for BATSRUS --

#ECHO
T                       DoEcho

#NORMALIZATION
SOLARWIND

#VERBOSE
1                       lVerbose

#PLANET
VENUS                      problem_type: 14=Venus Ionosphere

#ROTATION 
F

#GRAVITY
T
0

#INCLUDE
Param/VENUS/Grid

#GRIDLEVEL
2			nLevelArea
initial			NameArea

! The denisities are overwritten in the Venus ModUser file
#BODY
T			body1
1.0231328486450759	Rbody
1.0        		Rcurrents
1.0e6                 	BodyNDim Hp
1.0e6                 	BodyNDim CO2p
1.0e6                 	BodyNDim O2p
1.0e6                 	BodyNDim Op
300.0                 	BodyTdim

#TIMEACCURATE
F			DoTimeAccurate

#TIMESTEPPING
2		nStage
0.2		CflExpl

#USERFLAGS
F			UseUserInnerBcs
T			UseUserSource
F			UseUserPerturbation
F                       UseUserOuterBcs
T                       UseUserICs
F                       UseUserSpecifyRefinement
T                       UseUserLogFiles
F                       UseUserWritePlot
F                       UseUserAMR
F                       UseUserEchoInput
F                       UseUserB0
T                       UseUserInitSession
F                       UseUserUpdateStates

#TEST
read_inputs amr 
update_states calc_sources calc_facefluxes calc_timestep

#TESTXYZ
0.6672			xTest		(X coordinate of cell for testing)
-0.7864  		yTest		(Y coordinate of cell for testing)
-0.0169			zTest		(Z coordinate of cell for testing)

#TESTTIME
10			nIterTest	(iteration number to start testing)
10.5			TimeTest	(time to start testing in seconds) 
						Only read if timeaccurate!
#TESTVAR
1			iVarTest

! Index of variable to be tested. Default is rho_=1, ie. density.

#SAVELOGFILE
T                       DoSaveLogfile, rest of parameters read if true
VAR step                StringLog
1                       DnSaveLogfile
-1.                     DtSaveLogfile [sec]
dt pmin pmax rho p hpflx opflx o2pflx co2pflx
3.0 5.0

#BOXBOUNDARY
outflow                 TypeBc1
inflow                  TypeBc2
fixed                   TypeBc3
fixed                   TypeBc4
fixed                   TypeBc5
fixed                   TypeBc6

#INNERBOUNDARY
user

#MULTISPECIES
T			DoReplaceDensity
5.0			SpeciesPercentCheck

#SOLARWIND
14.0                    SW_rho_dim [n/cc]    \
2.703E5                 SW_T_dim [K]          |
-400.0                  SW_Ux_dim [km/s]      |  
0.0                     SW_Uy_dim [km/s]      |
0.0                     SW_Uz_dim [km/s]      |
0.0                     SW_Bx_dim [nT]        |
15.0                    SW_By_dim [nT]        !15 nT parker spiral
0.0                     SW_Bz_dim [nT]       /

#SAVERESTART
F

#SAVEPLOT
5			nplotfiles
z=0 MHD tec             StringPlot
1000			DnOutput
-1.0			DxOutput
y=0 MHD tec             StringPlot
1000			DnOutput
-1.0
x=0 MHD idl             StringPlot
1000			DnSaveplot
-1.0			DtSaveplot
-1.0
y=0 MHD idl             StringPlot
1000			DnSaveplot
-1.0			DtSaveplot
-1.0
z=0 MHD idl             StringPlot
1000			DnSaveplot
-1.0			DtSaveplot
-1.0			DxSaveplot

#SCHEME
2                       nOrder (1 or 2)
Linde                 FluxType  1=Roe, 2=Rusanov, 3=Linde
mc                  LimiterType ! Only for nordder=2
1.2

#STOP
50
-1.0

#END


#RUN ===============================================

#SCHEME
2                       nOrder (1 or 2)
Linde                 FluxType  1=Roe, 2=Rusanov, 3=Linde
minmod                  LimiterType ! Only for nordder=2

#STOP
10000
-1.0

#RUN ===============================================

#SCHEME
2                       nOrder (1 or 2)
Linde                 FluxType  1=Roe, 2=Rusanov, 3=Linde
mc                  LimiterType ! Only for nordder=2
1.2

#STOP
25000
-1.0

#RUN ===================================================

#SCHEME
2                       nOrder (1 or 2)
Linde                 FluxType  1=Roe, 2=Rusanov, 3=Linde
mc                  LimiterType ! Only for nordder=2
1.5

#STOP
30000
-1.0

#END
